{
  "name": "mkit-bundle-npm-package",
  "version": "1.0.0",
  "description": "Micro npm package project bundle",
  "keywords": [
    "mkit",
    "bundle",
    "micro",
    "npm",
    "package",
    "project",
    "boilerplate",
    "template",
    "starter"
  ],
  "files": [
    "lib",
    "src"
  ],
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:examples": "npm run build && node examples/",
    "test:prod": "cross-env BABEL_ENV=production npm run test",
    "lint": "eslint --fix src/**/*.js && prettier --write src/**/*.{js,json,md}",
    "precommit": "lint-staged",
    "build": "cross-env BABEL_ENV=production babel src --out-dir lib",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.js"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-minify": "^0.3.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2"
  },
  "author": "MK IT Ltd. <hi@mkit.io>",
  "homepage": "https://github.com/mkitio/mkit-bundle-npm-package#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkitio/mkit-bundle-npm-package.git"
  },
  "bugs": {
    "url": "https://github.com/mkitio/mkit-bundle-npm-package/issues"
  },
  "license": "MIT"
}
